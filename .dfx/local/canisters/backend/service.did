 record {
   body: blob;
   headers: vec http_header;
   status: nat;
 };
type http_header = 
 record {
   name: text;
   value: text;
 };
service : {
  get_pokemon_ditto: () -> (text);
  transform: (record {
                context: blob;
                response: http_request_result;
              }) -> (http_request_result) query;
}
